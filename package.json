{
  "name": "JoyouAdminPanel",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "watch": "NODE_ENV=development vite",
    "dev": "npm run development",
    "develop": "npm run development",
    "development": "NODE_ENV=development vite build --mode development",
