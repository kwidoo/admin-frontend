    "husky": "^9.0.11",
    "postcss": "8.4.13",
    "prettier": "^3.2.5",
    "rollup-plugin-postcss": "^4.0.2",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.8.27",
